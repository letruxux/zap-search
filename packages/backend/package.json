{
  "name": "backend",
  "module": "src/index.ts",
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "google-sr": "^4.0.0",
    "hono": "^4.5.8",
    "ky": "^1.7.1"
  },
  "scripts": {
    "start": "bun .",
    "compile": "bun build . --compile --target=bun-linux-x64 --outfile ./server",
    "compile:win": "bun build . --compile --target=bun-windows-x64 --outfile ./server.exe",
    "compile:all": "npm run compile && npm run compile:win"
  }
}
